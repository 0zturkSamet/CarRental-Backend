# Server Configuration
SERVER_PORT=9090

# Database Configuration
DB_URL=jdbc:postgresql://localhost:5432/prorent_db
DB_USERNAME=prorentuser
DB_PASSWORD=your_secure_password_here

# JPA/Hibernate Configuration
JPA_SHOW_SQL=false
JPA_DDL_AUTO=update

# JWT Configuration
# IMPORTANT: Generate a secure random string for production (at least 256 bits)
# Example: openssl rand -base64 64
JWT_SECRET=your_super_secure_jwt_secret_key_at_least_256_bits_long_change_this_in_production
JWT_EXPIRATION_MS=86400000

# CORS Configuration
CORS_ALLOWED_ORIGINS=http://localhost:3000,http://localhost:4200
CORS_ALLOWED_METHODS=GET,POST,PUT,DELETE,OPTIONS
CORS_ALLOWED_HEADERS=*
CORS_ALLOW_CREDENTIALS=true

# Logging Configuration
LOG_LEVEL_WEB=INFO
LOG_LEVEL_APP=DEBUG
LOG_LEVEL_HIBERNATE=WARN
